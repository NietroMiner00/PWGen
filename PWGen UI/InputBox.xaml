<Window x:Class="PWGen_UI.InputBox"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PWGen_UI"
        mc:Ignorable="d"
        Title="PWGen" Height="137" Width="305" Topmost="True" ResizeMode="NoResize" WindowStartupLocation="CenterScreen" ShowInTaskbar="False" WindowStyle="ToolWindow">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <PasswordBox x:Name="input" Height="23" Margin="10,36,10,0" VerticalAlignment="Top"/>
        <Label x:Name="text" Content="Passwort eingeben:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,10,0,0"/>
        <Button x:Name="ok" Content="Ok" Margin="0,64,90,10" IsDefault="True" Click="ok_Click" HorizontalAlignment="Right" Width="75"/>
        <Button Content="Abbrechen" Margin="0,64,10,10" IsCancel="True" HorizontalAlignment="Right" Width="75"/>

    </Grid>
</Window>
