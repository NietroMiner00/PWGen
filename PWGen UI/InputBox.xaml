<Window x:Class="PWGen_UI.InputBox"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PWGen_UI"
        mc:Ignorable="d"
        Title="PWGen" Height="133.404" Width="195.126" Topmost="True" ResizeMode="NoResize" WindowStartupLocation="CenterScreen" ShowInTaskbar="False" WindowStyle="ToolWindow">
    <Grid>
        <Label x:Name="text" Content="Passwort eingeben:" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="0,10,0,0"/>
        <Button x:Name="ok" Content="Ok" HorizontalAlignment="Left" Margin="10,64,0,0" VerticalAlignment="Top" Width="75" IsDefault="True" Click="ok_Click"/>
        <TextBox x:Name="input" HorizontalAlignment="Left" Height="23" Margin="10,36,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="155" ForceCursor="True"/>
        <Button Content="Abbrechen" HorizontalAlignment="Left" Margin="90,64,0,0" VerticalAlignment="Top" Width="75" IsCancel="True"/>

    </Grid>
</Window>
